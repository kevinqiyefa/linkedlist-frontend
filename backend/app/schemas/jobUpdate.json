{
  "$id": "http://example.com/example.json",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "title": {
      "$id": "/properties/title",
      "type": "string",
      "title": "The Title Schema ",
      "default": "",
      "minLength": 1,
      "maxLength": 55,
      "examples": ["Lead Instructor"]
    },
    "salary": {
      "$id": "/properties/salary",
      "type": "string",
      "title": "The Salary Schema ",
      "default": "",
      "minLength": 1,
      "maxLength": 55,
      "examples": ["1000000000"]
    },
    "equity": {
      "$id": "/properties/equity",
      "type": "number",
      "title": "The Equity Schema ",
      "default": 0,
      "examples": [0.5]
    },
    "company": {
      "$id": "/properties/company",
      "type": "string",
      "title": "The Company Schema ",
      "default": "",
      "examples": ["rithm"]
    }
  }
}
