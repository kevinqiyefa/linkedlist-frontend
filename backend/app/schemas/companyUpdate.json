{
  "$id": "http://example.com/example.json",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "name": {
      "$id": "/properties/name",
      "type": "string",
      "title": "The Name Schema ",
      "default": "",
      "minLength": 1,
      "maxLength": 55,
      "examples": ["Rithm"]
    },
    "handle": {
      "$id": "/properties/handle",
      "type": "string",
      "title": "The Handle Schema ",
      "default": "",
      "minLength": 1,
      "maxLength": 55,
      "examples": ["rithm"]
    },
    "email": {
      "$id": "/properties/email",
      "type": "string",
      "title": "The Email Schema ",
      "default": "",
      "minLength": 6,
      "maxLength": 60,
      "format": "email",
      "examples": ["test@rithmschool.com"]
    },
    "password": {
      "$id": "/properties/password",
      "type": "string",
      "title": "The Password Schema ",
      "default": "",
      "minLength": 5,
      "maxLength": 10,
      "examples": ["foo123"]
    },
    "logo": {
      "$id": "/properties/logo",
      "type": "string",
      "title": "The Logo Schema ",
      "default": "",
      "format": "uri",
      "examples": [
        "https://ih1.redbubble.net/image.396326354.4958/flat,800x800,075,t.u1.jpg"
      ]
    }
  }
}
